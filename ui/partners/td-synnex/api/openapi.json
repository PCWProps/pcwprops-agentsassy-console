  "info": {
    "title": "TD SYNNEX Partner API",
    "description": "These APIs provide comprehensive access to product information, order management, and other business operations for seamless integration with your applications. With these services, you can perform price and availability queries, submit purchase orders, manage invoices, and more, all in real time.\n",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://api-uat.us.tdsynnex.com",
      "description": "Sandbox Server US"
    },
    {
      "url": "https://api-uat.ca.tdsynnex.com",
      "description": "Sandbox Server CA"
    },
    {
      "url": "https://api.us.tdsynnex.com",
      "description": "Production Server US"
    },
    {
      "url": "https://api.ca.tdsynnex.com",
      "description": "Production Server CA"
    }
  ],
  "tags": [
    {
      "name": "Authentication",
      "description": "OAuth2 authentication endpoints"
    },
    {
      "name": "Quotes",
      "description": "Endpoints related to quote queries"
    },
    {
      "name": "Orders",
      "description": "Endpoints related to order processing and queries"
    },
    {
      "name": "Invoices",
      "description": "Endpoints for accessing invoice information and reports"
    }
  ],
  "paths": {
    "/oauth2/v1/token": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Get Access Token",
        "description": "Obtain an OAuth2 access token using client credentials.\n\nUse this token in the `Authorization` header for all subsequent API requests.\n\n**Token Lifetime:** 7200 seconds (2 hours)\n",
        "operationId": "AUTH_POST_oauth2Token",
        "servers": [
          {
            "url": "https://sso.us.tdsynnex.com",
            "description": "Authorization Server"
          }
        ],
        "security": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "required": [
                  "grant_type",
                  "client_id",
                  "client_secret"
                ],
                "properties": {
                  "grant_type": {
                    "type": "string",
                    "enum": [
                      "client_credentials"
                    ],
                    "description": "Must be 'client_credentials'",
                    "example": "client_credentials"
                  },
                  "client_id": {
                    "type": "string",
                    "description": "Your client identifier",
                    "example": "YOUR_CLIENT_ID"
                  },
                  "client_secret": {
                    "type": "string",
                    "description": "Your client secret (keep secure)",
                    "example": "YOUR_CLIENT_SECRET"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Access token successfully generated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string",
                      "description": "The access token to use in API requests",
                      "example": "eyJ4NXQiOiJyNVFpNmlWenRtVjgxZ2JFcUpIVTdiTE..."
                    },
                    "token_type": {
                      "type": "string",
                      "description": "Token type (always 'Bearer')",
                      "example": "Bearer"
                    },
                    "expires_in": {
                      "type": "integer",
                      "description": "Token lifetime in seconds",
                      "example": 7200
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request - Invalid parameters or client credentials",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "invalid_client"
                    },
                    "error_description": {
                      "type": "string",
                      "example": "Client authentication failed"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
